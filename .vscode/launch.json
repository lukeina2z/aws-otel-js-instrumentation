    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug AWS ADOT Tests",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/.bin/ts-mocha",
            "args": [
                "--timeout",
                "10000",
                "-p",
                "tsconfig.json",
                "--require",
                "@opentelemetry/contrib-test-utils",
                "test/**/*.ts"
            ],
            "cwd": "${workspaceFolder}/aws-distro-opentelemetry-node-autoinstrumentation",
            "console": "integratedTerminal",
            "sourceMaps": true,
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/aws-distro-opentelemetry-node-autoinstrumentation/build/**/*.js"
            ],
            "env": {
                "NODE_OPTIONS": "--enable-source-maps"
            }
        },
        {
            "name": "zADOT-Launch-Node",
            "cwd": "${workspaceFolder}/zADOT",
            "program": "${workspaceFolder}/zADOT/index.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "env": {
                "NODE_OPTIONS": "--require @aws/aws-distro-opentelemetry-node-autoinstrumentation/register",
                "OTEL_LOG_LEVEL": "all",
                //"OTEL_RESOURCE_ATTRIBUTES": "service.name=ADOT-Nodejs",
                "OTEL_SERVICE_NAME": "zADOT-Nodejs-Launch-Node",
                "OTEL_NODE_RESOURCE_DETECTORS": "env,host,os,process",
                "OTEL_EXPORTER_OTLP_TRACES_TIMEOUT": "3",
                "OTEL_METRICS_EXPORTER": "none",
                "OTEL_LOGS_EXPORTER": "none",
                // "OTEL_TRACES_EXPORTER": "otlp,console",
                "OTEL_AWS_APPLICATION_SIGNALS_ENABLED": "true",
                "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
                "OTEL_TRACES_SAMPLER": "always_on", // "xray",
                // "OTEL_TRACES_SAMPLER_ARG": "endpoint=http://localhost:2000",
                "OTEL_AWS_APPLICATION_SIGNALS_EXPORTER_ENDPOINT": "http://127.0.0.1:4316/v1/metrics",
                "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://127.0.0.1:4318/v1/traces"
            },
            "sourceMaps": true,
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "!**/node_modules/**"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "zADOT-NPM-Run-Script",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "debug"
            ],
            "cwd": "${workspaceFolder}/zADOT",
            "runtimeExecutable": "npm",
            "skipFiles": [
                // "<node_internals>/**"
            ],
            "type": "node",
            "env": {
                // "OTEL_LOG_LEVEL": "all",
                //"OTEL_RESOURCE_ATTRIBUTES": "service.name=ADOT-Nodejs",
                "OTEL_SERVICE_NAME": "zADOT-Nodejs-Launch-Npm",
                "OTEL_NODE_RESOURCE_DETECTORS": "env,host,os,process",
                // "NODE_OPTIONS": "--require @aws/aws-distro-opentelemetry-node-autoinstrumentation/register",
                "OTEL_EXPORTER_OTLP_TRACES_TIMEOUT": "3",
                "OTEL_METRICS_EXPORTER": "none",
                "OTEL_LOGS_EXPORTER": "none",
                // "OTEL_TRACES_EXPORTER": "otlp,console",
                "OTEL_AWS_APPLICATION_SIGNALS_ENABLED": "true",
                "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
                "OTEL_TRACES_SAMPLER": "always_on", // "xray",
                // "OTEL_TRACES_SAMPLER_ARG": "endpoint=http://localhost:2000",
                "OTEL_AWS_APPLICATION_SIGNALS_EXPORTER_ENDPOINT": "http://127.0.0.1:4316/v1/metrics",
                "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://127.0.0.1:4318/v1/traces"
            },
            "sourceMaps": true,
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "/Users/lukezha/github/playground/AppSignalsTestApps/app-signals-nodejs-ADOT/node_modules/@aws/aws-distro-opentelemetry-node-autoinstrumentation/build/**/*.js",
                "/Users/lukezha/github/otel-js/working/aws-otel-js-instrumentation-lkmain/aws-distro-opentelemetry-node-autoinstrumentation/build/**/*.js",
                "!**/node_modules/**"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "z-Sample",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "debug",
                "--workspace=z-examples/adot-js-lambda"
            ],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "console": "integratedTerminal",
            "env": {
                "OTEL_RESOURCE_ATTRIBUTES": "service.name=ADOT-JS-Lambda-Local-XY",
                "DEBUG": "*",
                // "OTEL_LOG_LEVEL": "none",
                "OTEL_LOG_LEVEL": "all",
                // "AWS_REGION": "us-west-2",
                // "NODE_OPTIONS": "--enable-source-maps",
                // "OTEL_NODE_RESOURCE_DETECTORS": "aws,env,host,os,process,container", // "all",
                /// Metric
                // "OTEL_METRICS_EXPORTER": "none",
                // "OTEL_METRIC_EXPORT_INTERVAL": "2000",
                /// Log
                // "OTEL_LOGS_EXPORTER": "otlp",
                "OTEL_LOGS_EXPORTER": "console,otlp",
                /// Trace
                "OTEL_TRACES_EXPORTER": "otlp",
                // "OTEL_TRACES_EXPORTER": "console,otlp",
                "OTEL_NODE_ENABLED_INSTRUMENTATIONS": "aws-sdk, http, https, pino, winston",
                // "OTEL_NODE_ENABLED_INSTRUMENTATIONS": "aws-sdk, fs,http,nestjs-core",
                /// OTLP Exporter
                "OTEL_EXPORTER_OTLP_ENDPOINT": "http://localhost:4318",
                // "OTEL_EXPORTER_OTLP_PROTOCOL": "http/json", // default is protobuf
                "OTEL_EXPORTER_OTLP_PROTOCOL": "http/protobuf",
                // "OTEL_EXPORTER_OTLP_TRACES_ENDPOINT": "http://localhost:4318/v1/traces",
                /// AWS Application Signals
                // "OTEL_TRACES_SAMPLER": "always_on",
                // "OTEL_TRACES_SAMPLER_ARG": "endpoint=http://localhost:2000",
                // "OTEL_AWS_APPLICATION_SIGNALS_RUNTIME_ENABLED": "false",
                "OTEL_AWS_APPLICATION_SIGNALS_ENABLED": "true",
                "OTEL_AWS_APPLICATION_SIGNALS_EXPORTER_ENDPOINT": "http://localhost:4318/v1/metrics",
                // "OTEL_INSTRUMENTATION_AWS_SDK_EXPERIMENTAL_SPAN_ATTRIBUTES": "true"
            },
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/z-examples/adot-js-lambda/**/*.js"
            ],
            "resolveSourceMapLocations": [
                "${workspaceFolder}/aws-distro-opentelemetry-node-autoinstrumentation/**",
                "${workspaceFolder}/node_modules/@opentelemetry/instrumentation-winston/**",
                "${workspaceFolder}/../opentelemetry-js-contrib/packages/auto-instrumentations-node/**",
                "${workspaceFolder}/../opentelemetry-js-contrib/packages/instrumentation-aws-sdk/**",
                "${workspaceFolder}/../opentelemetry-js/api/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/api-logs/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/opentelemetry-sdk-node/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/opentelemetry-instrumentation/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/opentelemetry-instrumentation-http/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/otlp-exporter-base/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/exporter-trace-otlp-http/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/exporter-trace-otlp-proto/**",
                "${workspaceFolder}/../opentelemetry-js/packages/opentelemetry-core/**",
                "${workspaceFolder}/../opentelemetry-js/packages/opentelemetry-context-async-hooks/**",
                "${workspaceFolder}/../opentelemetry-js/packages/opentelemetry-sdk-trace-base/**",
                "${workspaceFolder}/../opentelemetry-js/packages/opentelemetry-sdk-trace-node/**",
                "!**/node_modules/**"
            ],
            // "sourceMapPathOverrides": {
            //     "../../src/instrumentation.ts": "/Users/lukezha/github/otel-js/99bld/opentelemetry-js-contrib/packages/instrumentation-winston/src/instrumentation.ts",
            //     "../../src/register.ts": "/Users/lukezha/github/otel-js/99bld/opentelemetry-js-contrib/packages/auto-instrumentations-node/src/register.ts",
            // }
        },
        {
            "name": "ut-dbg",
            "request": "launch",
            "runtimeArgs": [
                "run-script",
                "test"
            ],
            "cwd": "${workspaceFolder}",
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "env": {
                "NODE_OPTIONS": "--enable-source-maps",
                "OTEL_LOG_LEVEL": "none"
            },
            "sourceMaps": true,
            "console": "integratedTerminal",
            "resolveSourceMapLocations": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/../opentelemetry-js/api/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/api-logs/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/opentelemetry-sdk-node/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/opentelemetry-instrumentation/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/exporter-trace-otlp-http/**",
                "${workspaceFolder}/../opentelemetry-js/experimental/packages/exporter-trace-otlp-proto/**",
                "${workspaceFolder}/../opentelemetry-js/packages/opentelemetry-core/**",
                "${workspaceFolder}/../opentelemetry-js/packages/opentelemetry-context-async-hooks/**",
                "${workspaceFolder}/../opentelemetry-js/packages/opentelemetry-sdk-trace-base/**",
                "${workspaceFolder}/../opentelemetry-js/packages/opentelemetry-sdk-trace-node/**",
                "!**/node_modules/**"
            ],
            // "sourceMapPathOverrides": {
            //     "../../src/instrumentation.ts": "${workspaceFolder}/../opentelemetry-js-contrib/plugins/node/instrumentation-fs/src/instrumentation.ts",
            //     "${workspaceFolder}/../opentelemetry-js-contrib/plugins/node/instrumentation-fs/src/instrumentation.ts": "../../src/instrumentation.ts",
            //     "${workspaceFolder}/examples/basic-tracer-node/node_modules/@opentelemetry/*": "${workspaceFolder}/node_modules/@opentelemetry/*",
            // },
            "type": "node"
        }
    ]
}